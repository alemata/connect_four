<% if @info[:played] %>
  $('#board').html('<%= escape_javascript(render("board")) %>');
  $('#to_play').empty()
  <% if @info[:win] %>
    $('#turn').text("You winn!")
  <% else %>
    $('#turn').text("Wait your turn to play!")
  <% end %>
<% else %>
  $('#turn').text("Could not play... try again!")
<% end %>

